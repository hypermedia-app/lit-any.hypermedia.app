{
    "extends": "airbnb-base",
    "root": true,
    "rules": {
        "no-console": 0,
        "indent": [ "error", 4, { "SwitchCase": 1 } ],
        "no-underscore-dangle": "off",
        "no-trailing-spaces":"error",
        "import/no-extraneous-dependencies": "off",
        "import/first": "off",
        "no-alert": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    }
}
